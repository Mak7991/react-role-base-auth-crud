	"scripts": {
		"start": "yarn run tailwind && react-scripts start",
		"start-qa": "yarn run tailwind && env-cmd -f .env.qa react-scripts start",
		"start-stage": "yarn run tailwind && env-cmd -f .env.stage react-scripts start",
		"start-prod": "yarn run tailwind && env-cmd -f .env.prod react-scripts start",
		"build": "yarn run tailwind && yarn run purge-tailwind && cross-env GENERATE_SOURCEMAP=false react-scripts build",
		"build-qa": "yarn run tailwind && yarn run purge-tailwind && cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.qa react-scripts build",
		"build-stage": "yarn run tailwind && yarn run purge-tailwind && cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.stage react-scripts build",
		"build-prod": "yarn run tailwind && yarn run purge-tailwind && cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.prod react-scripts build",
		"tailwind": "tailwind build ./src/styles/tailwind-config.css -c ./tailwind.config.js -o ./src/styles/tailwind.css",
		"purge-tailwind": "node ./purge-tailwindcss.js",
		"test": "react-scripts test --env=node",
		"eject": "react-scripts eject",
		"build-docs": "find ./src/app/main/documentation/material-ui-components \\( -name '*.ts' -o -name '*.tsx' \\) -type f -delete && babel-node src/app/main/documentation/material-ui-components/build.js",
		"analyze": "yarn run tailwind && yarn run purge-tailwind && react-scripts build && source-map-explorer 'build/static/js/*.js' --html analyze-result.html",
		"pnpify-sdk": "yarn pnpify --sdk",
		"lint": "eslint"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version",
			"ie 11"
		]
	}
}
